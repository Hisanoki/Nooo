local function CreateSkillRow(skillKey, position)
    local frame = Instance.new("Frame") frame.Parent = ScrollFrame frame.BackgroundColor3 = Color3.fromRGB(0, 80, 140) frame.Position = position frame.Size = UDim2.new(0.9, 0, 0, 42)
    local corner = Instance.new("UICorner") corner.CornerRadius = UDim.new(0, 7) corner.Parent = frame
    
    local toggle = Instance.new("TextButton") toggle.Parent = frame toggle.BackgroundColor3 = CONFIG.SelectedSkills[skillKey] and Color3.fromRGB(0, 120, 80) or Color3.fromRGB(0, 60, 110) toggle.Position = UDim2.new(0.05, 0, 0.18, 0) toggle.Size = UDim2.new(0.35, 0, 0.64, 0) toggle.Font = Enum.Font.GothamBold toggle.Text = "Skill " .. skillKey toggle.TextColor3 = CONFIG.SelectedSkills[skillKey] and Color3.fromRGB(80, 255, 80) or Color3.fromRGB(255, 80, 80) toggle.TextSize = 13
    local corner1 = Instance.new("UICorner") corner1.CornerRadius = UDim.new(0, 5) corner1.Parent = toggle

    local input = Instance.new("TextBox") input.Parent = frame input.BackgroundColor3 = Color3.fromRGB(0, 100, 180) input.Position = UDim2.new(0.45, 0, 0.18, 0) input.Size = UDim2.new(0.5, 0, 0.64, 0) input.Font = Enum.Font.Gotham input.Text = tostring(CONFIG.SkillRanges[skillKey]) input.TextColor3 = Color3.fromRGB(255, 255, 255) input.TextSize = 13 input.PlaceholderText = "Range TP"
    local corner2 = Instance.new("UICorner") corner2.CornerRadius = UDim.new(0, 5) corner2.Parent = input

    toggle.MouseButton1Click:Connect(function()
        CONFIG.SelectedSkills[skillKey] = not CONFIG.SelectedSkills[skillKey]
        toggle.TextColor3 = CONFIG.SelectedSkills[skillKey] and Color3.fromRGB(80, 255, 80) or Color3.fromRGB(255, 80, 80)
        toggle.BackgroundColor3 = CONFIG.SelectedSkills[skillKey] and Color3.fromRGB(0, 120, 80) or Color3.fromRGB(0, 60, 110)
    end)

    input.FocusLost:Connect(function()
        local val = tonumber(input.Text)
        if val and val > 0 then
            CONFIG.SkillRanges[skillKey] = val
        else
            input.Text = tostring(CONFIG.SkillRanges[skillKey])
        end
    end)

    return toggle, input
end

local SkillR, InputR = CreateSkillRow("R", UDim2.new(0.05, 0, 0, yPos)) yPos = yPos + 48
